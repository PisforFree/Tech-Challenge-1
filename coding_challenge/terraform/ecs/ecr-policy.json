{
  "Version": "2008-10-17",
  "Statement": [
    {
      "Sid": "AllowPushPullForJenkinsUser",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::803767876973:user/new_admin_user"
      },
      "Action": [
        "ecr:GetDownloadUrlForLayer",
        "ecr:BatchGetImage",
        "ecr:BatchCheckLayerAvailability",
        "ecr:PutImage",
        "ecr:InitiateLayerUpload",
        "ecr:UploadLayerPart",
        "ecr:CompleteLayerUpload"
      ],
      "Resource": "arn:aws:ecr:us-east-1:803767876973:repository/my-application-repo"
    }
  ]
}


